<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fr_content_three"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".content.IntroductionOneFragment">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_gravity="center|center_horizontal">


        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="100dp"
            android:background="@drawable/landing_page3" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_landingpage3"
            android:layout_width="400dp"
            android:layout_height="40dp"
            android:layout_gravity="center_horizontal"
            android:fontFamily="@font/comicneue_bold"
            android:gravity="center"
            android:text="@string/tv_lamdingpage3"
            android:textSize="30dp" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/et_nama"
            android:layout_width="350dp"
            android:layout_height="75dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:maxLines="1"
            android:layout_marginRight="20dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_mulai"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="0dp"
            android:layout_marginRight="20dp"
            android:backgroundTint="@color/black"
            android:text="next"
            android:textColor="@android:color/white" />




    </androidx.appcompat.widget.LinearLayoutCompat>

</FrameLayout>